<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="wrap_content" android:background="@drawable/selector_of_default_item">

    <ImageView
        android:id="@+id/upLine_in_default_item"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        style="@style/normal_divider"
        android:visibility="gone"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_in_infoActivity_item"
            android:layout_width="@dimen/width_of_item_in_infoActivity"
            android:layout_height="@dimen/height_of_leftItem_in_infoActivity"
            android:layout_weight="0"
            android:text="@string/string_of_enterpriseName"
            style="@style/leftItem_in_registerActivity"/>

        <com.sintn.hera.mobile.cash.widget.view.EditTextWithFrame
            android:id="@+id/et_in_infoActivity_item"
            android:layout_width="0dp"
            android:layout_height="@dimen/height_of_leftItem_in_infoActivity"
            android:layout_weight="1"
            android:hint="@string/string_of_enterEnterpriseName"
            style="@style/et_in_infoActivity"/>

    </LinearLayout>

    <ImageView
        android:id="@+id/short_downLine_in_default_item"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        style="@style/margin_divider"/>

    <ImageView
        android:id="@+id/long_downLine_in_default_item"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        style="@style/normal_divider"
        android:visibility="gone"/>

</LinearLayout><!-- From: file:/E:/Code/收款宝/worker/HuiyuantongVenusMobileCash-V3.x/app/src/main/res/layout/default_item_in_info_activity_has_edit.xml -->