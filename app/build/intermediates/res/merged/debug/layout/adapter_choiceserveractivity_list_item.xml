<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/selector_white_f0f0f0"
    android:orientation="horizontal"
    android:paddingLeft="10dp"
    android:paddingRight="10dp" >

    <TextView
        android:id="@+id/tv_in_choiceserveractivity_for_adapter_num"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_margin="9.6dp"
        android:layout_weight="1"
        android:focusable="false"
        android:gravity="center"
        android:maxLength="120"
        android:singleLine="true"
        android:textColor="#000000"
        android:textSize="14sp" />

    <View
        android:layout_width="0.5dp"
        android:layout_height="match_parent"
        android:layout_margin="9.6dp"
        android:background="#d4d6d9"
        android:focusable="false" />

    <TextView
        android:id="@+id/tv_in_choiceserveractivity_for_adapter_desc"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_margin="9.6dp"
        android:layout_weight="5"
        android:focusable="false"
        android:gravity="center_vertical"
        android:singleLine="true"
        android:textColor="#838891"
        android:textSize="14sp"
        tools:ignore="HardcodedText" />

